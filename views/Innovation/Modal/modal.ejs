<div class="modal fade" id="modalUpdatePart" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xlg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalLabel"><strong>Cập nhật thông tin vật tư</strong></h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="" style="background-color: #28a745; height: 3px"></div>
            <div class="modal-body">
                <div class="row">
                    <input type="hidden" id="txtDPartId">
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Tên vật tư <span class="text-danger">*</span></label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartName" autofocus>
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">ID nhà máy <span class="text-danger">*</span></label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartVendorCode">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Mã vật tư MEC <span class="text-danger">*</span></label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartCode">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Số lượng</label>
                        <div class="">
                            <input type="number" class="form-control" id="txtDPartQty" value="0">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Số lượng tối thiểu</label>
                        <div class="">
                            <input type="number" class="form-control" id="txtDPartMinQty" value="0">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Vị trí</label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartLocation">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Đơn giá</label>
                        <div class="">
                            <input type="number" class="form-control" id="txtDPartPrice" value="0">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Đơn vị</label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartUnit">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Model</label>
                        <div class="w-100">
                            <select id="txtPartModel" class="form-control list-model" onchange="changeModel()">
                            </select>
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3">
                        <label for="">Mô tả</label>
                        <div class="">
                            <input type="text" class="form-control" id="txtDPartDes">
                        </div>
                    </div>
                    <div class="form-group col-sm-12 col-md-3 d-flex">
                        <label for="" class="mr-3">Ảnh</label>
                        <div class="">
                            <label class="d-flex align-items-center justify-content-center" style="cursor: pointer;">
                                <input type="hidden" id="d-old-part-img">
                                <img id="d-part-img" src="/Image/upload.png" width="150px" height="150px" loading="lazy">
                                <input type="file" accept=".jpg, .jpeg, .png" id="d-part-image-upload" onchange="uploadImage(event)" class="d-none">
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-success" onclick="updatePart()">Update</button>
                </div>
            </div>
        </div>
    </div>
</div>